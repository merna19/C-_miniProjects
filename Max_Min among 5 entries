class myclass
{
    public static void Main()
    {
        //read the 3 coeffecient
        console.WriteLine("Enter coeffecient a: ");
        int a = int.parse(console.ReadLine());
        console.WriteLine("Enter coeffecient b: ");
        int b = int.parse(console.ReadLine());
        console.WriteLine("Enter coeffecient c: ");
        int c = int.parse(console.ReadLine());

        //check the root
        int underRoot = 4 * a * c;

        if (underRoot > 0)
        {
            console.WriteLine("the 2 roots are Real");
        }
        else
        {
            if (underRoot < 0)
            {
                console.WriteLine("the 2 roots are Imaginary");
            }
            else
            {
                console.WriteLine("the 2 roots are Equal);
           }
        }
    }
}
